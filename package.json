{
  "name": "nodejs-shopping-cart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/server",
    "knex:make": "knex migrate:make --knexfile db/knexfile.js",
    "knex:migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "knex:rollback": "knex migrate:rollback --knexfile db/knexfile.js",
    "knex:makeseed": "knex seed:make --knexfile db/knexfile.js",
    "knex:seed": "knex seed:run --knexfile db/knexfile.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "knex": "^0.16.3",
    "morgan": "~1.9.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
